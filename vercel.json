{
  "rewrites": [
    {
      "source": "/serve-ad/:listingId",
      "destination": "/api/serve-ad/[listingId]"
    },
    {
      "source": "/serve-campaign/:campaignId",
      "destination": "/api/serve-campaign/[campaignId]"
    },
    {
      "source": "/api/track-click",
      "destination": "/api/track-click.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/|serve-ad/|serve-campaign/).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/serve-ad/[listingId].js": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/serve-campaign/[campaignId].js": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/track-click.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/serve-ad/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW"
        }
      ]
    },
    {
      "source": "/api/check-ad-status",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}